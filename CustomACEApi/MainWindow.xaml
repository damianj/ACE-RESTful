<Window x:Class="CustomACEAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomACEAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="700">
    <Grid Background="#FF042E3A">
        <Button Content="START" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="332" Height="41" FontSize="24" FontWeight="Bold" Background="#FF042E3A" Foreground="White" BorderBrush="White" Click="Start_APP"/>
        <RichTextBox HorizontalAlignment="Left" Height="294" Margin="10,56,0,0" VerticalAlignment="Top" Width="672" FontFamily="Consolas" FontWeight="Bold" Background="#19FFFFFF" Foreground="White" SelectionBrush="#FF85BDCD" TextChanged="RichTextBox_TextChanged">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button Content="QUIT" HorizontalAlignment="Left" Margin="350,10,0,0" VerticalAlignment="Top" Width="332" Height="41" FontWeight="Bold" FontSize="24" Background="#FF042E3A" Foreground="White" BorderBrush="White" Click="Stop_APP"/>
        <StatusBar HorizontalAlignment="Left" Height="19" Margin="10,350,0,0" VerticalAlignment="Top" Width="672" Background="#FF042E3A" BorderBrush="White" Foreground="White"/>

    </Grid>
</Window>
